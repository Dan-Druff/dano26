{
  "imports": {
    "hono": "jsr:@hono/hono@^4.11.4",
    "hono/jsx": "jsr:@hono/hono@^4.11.4/jsx",
    "argon2": "npm:argon2"
  },
  "tasks": {
    "start": "deno run --allow-net --unstable-kv main.tsx",
    "dev":"deno run --allow-ffi --allow-net --allow-read --unstable-kv --allow-env --allow-sys --watch main.tsx"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
